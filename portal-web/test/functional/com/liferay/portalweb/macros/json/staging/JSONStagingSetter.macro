definition {

	macro setAttributes {
		var portletIds = "";
		var portletIds = List.add(item = "com_liferay_blogs_web_portlet_BlogsAdminPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_bookmarks_web_portlet_BookmarksAdminPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_calendar_web_portlet_CalendarAdminPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_document_library_web_portlet_DLAdminPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_dynamic_data_lists_web_portlet_DDLPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_dynamic_data_mapping_data_provider_web_portlet_DDMDataProviderPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_dynamic_data_mapping_form_web_portlet_DDMFormAdminPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_dynamic_data_mapping_web_portlet_PortletDisplayTemplatePortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_journal_web_portlet_JournalPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_knowledge_base_web_portlet_AdminPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_message_boards_web_portlet_MBAdminPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_mobile_device_rules_web_portlet_MDRPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_polls_web_portlet_PollsPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_segments_web_internal_portlet_SegmentsPortlet", list = "${portletIds}");
		var portletIds = List.add(item = "com_liferay_wiki_web_portlet_WikiAdminPortlet", list = "${portletIds}");

		var attributes = "";

		for (var portletId : list "${portletIds}") {
			var result = JSONPortlet.hasPortlet(portletId = "${portletId}");

			var attribute = '''"staged--staged-portlet_${portletId}--" : "true"''';

			if ("${result}" == "true") {
				var attributes = List.add(
					item = "${attribute}",
					list = "${attributes}");
			}
		}

		var attributes = '''{${attributes}}''';

		return "${attributes}";
	}

}