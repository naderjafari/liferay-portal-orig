				portletName = "Blogs",
				targetFragmentName = "Asset Publisher");

			Refresh();

			PageEditor.addWidget(
				portletName = "Documents and Media",
				targetFragmentName = "Blogs");

			Refresh();

			PageEditor.addWidget(
				portletName = "My Sites",
				targetFragmentName = "Documents and Media");
